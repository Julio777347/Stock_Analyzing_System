<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>customer</servlet-name>
        <servlet-class>stocking.servlet.Customer_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>singleStock</servlet-name>
        <servlet-class>stocking.servlet.SingleStock_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>market</servlet-name>
        <servlet-class>stocking.servlet.Market_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>strategy</servlet-name>
        <servlet-class>stocking.servlet.Strategy_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bgraph</servlet-name>
        <servlet-class>stocking.servlet.BGraph_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>collection</servlet-name>
        <servlet-class>stocking.servlet.CustomerCollection_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>customer</servlet-name>
        <url-pattern>/customer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>singleStock</servlet-name>
        <url-pattern>/singleStock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>market</servlet-name>
        <url-pattern>/market</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>strategy</servlet-name>
        <url-pattern>/strategy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bgraph</servlet-name>
        <url-pattern>/bgraph</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>collection</servlet-name>
        <url-pattern>/collection</url-pattern>
    </servlet-mapping>

</web-app>
